{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,  // Garantir que o TypeScript seja sensível a maiúsculas e minúsculas
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "*": ["node_modules/*", "../server/src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
